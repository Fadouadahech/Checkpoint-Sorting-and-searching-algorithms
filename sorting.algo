ALGORITHM insertion_sort
VAR
// this is the declartion of variables used in the problem 
    i , j , a , n = integer
    T : ARRAY_OF integer[n]
  // this is the body section 
BEGIN
    for i from 1 to n do
    a:=T[i]
    j:= i
    while (j>1)and (T[j-1]>a)do
    T[j]:= T[j-1]
    j:=j-1;
    end while 
    T[j]:=a 
    end for 
    
END